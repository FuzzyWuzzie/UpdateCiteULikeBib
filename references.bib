    citeulike-linkout-1 = {http://view.ncbi.nlm.nih.gov/pubmed/22300480},
    citeulike-linkout-2 = {http://www.hubmed.org/display.cgi?uids=22300480},
    doi = {10.1080/10255842.2011.627682},
    issn = {1476-8259},
    journal = {Computer methods in biomechanics and biomedical engineering},
    keywords = {aetiology, dti},
    number = {5},
    pages = {520--529},
    pmid = {22300480},
    posted-at = {2014-01-07 21:51:47},
    priority = {2},
    title = {How does muscle stiffness affect the internal deformations within the soft tissue layers of the buttocks under constant loading?},
