    doi = {10.1115/1.2800830},
    issn = {0148-0731},
    journal = {Journal of biomechanical engineering},
    keywords = {properties, viscoelastic},
    month = dec,
    number = {6},
    pages = {924--930},
    pmid = {18067398},
    posted-at = {2013-11-26 00:17:03},
    priority = {2},
    title = {Viscoelastic properties of ovine adipose tissue covering the gluteus muscles.},
